{
  "name": "xiaosongshu/fasterphpweb",
  "description": "一个常驻内存 php框架，提供http，rtmp，websocket服务",
  "type": "project",
  "license": "MIT",
  "keywords": [
    "http service",
    "rtmp service",
    "websocket service"
  ],
  "autoload": {
    "psr-4": {
      "MediaServer\\": "src",
      "Root\\": "root",
      "Process\\": "process",
      "Ws\\": "ws",
      "App\\": "app"
    },
    "files": [
      "./app/functions.php"
    ]
  },
  "authors": [
    {
      "name": "yanglong",
      "email": "2723659854@qq.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=8.1",
    "php-amqplib/php-amqplib": "3.2.*",
    "elasticsearch/elasticsearch": "7.17.*",
    "xiaosongshu/colorword": "dev-master",
    "phpseclib/phpseclib": "^3.0",
    "jeremeamia/superclosure": "^2.4@dev",
    "nikic/fast-route": "^2.0@dev",
    "ext-pcntl": "*",
    "ext-mbstring": "*",
    "ext-posix": "*",
    "ext-sockets": "*",
    "ext-mysqli": "*",
    "evenement/evenement": "^3.0",
    "apix/log": "^1.2",
    "react/promise": "^2.9",
    "xiaosongshu/nacos": "dev-master",
    "symfony/yaml": "5.4.*",
    "psr/container": "^1.1.1",
    "php-di/php-di": "^6",
    "doctrine/annotations": "^1.14",
    "xiaosongshu/mail": "dev-master",
    "ext-gd": "*",
    "ext-memcache": "*",
    "nikic/php-parser": "^4.19",
    "ext-zlib": "*",
    "haozi-team/chatgpt-php": "^2.1",
    "ext-readline": "*",
    "react/event-loop": "^3@dev",
    "nmred/kafka-php": "0.2.*",
    "monolog/monolog": "^2.9",
    "xiaosongshu/progress": "^1.0",
    "xiaosongshu/table": "^1.0",
    "ext-redis": "*"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
