{
  "name": "xiaosongshu/fasterphpweb",
  "description": "一个常驻内存 php框架，提供http，rtmp，websocket服务",
  "type": "project",
  "license": "MIT",
  "keywords": [
    "http service",
    "rtmp service",
    "websocket service"
  ],
  "autoload": {
    "psr-4": {
      "MediaServer\\": "src"
    }
  },
  "authors": [
    {
      "name": "yanglong",
      "email": "2723659854@qq.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=8.1",
    "php-amqplib/php-amqplib": "3.2.*",
    "elasticsearch/elasticsearch": "7.17.*",
    "xiaosongshu/colorword": "dev-master",
    "xiaosongshu/progress": "dev-master",
    "xiaosongshu/table": "dev-master",
    "phpseclib/phpseclib": "*",
    "jeremeamia/superclosure": "^2.4@dev",
    "nikic/fast-route": "^2.0@dev",
    "ext-pcntl": "*",
    "ext-mbstring": "*",
    "ext-posix": "*",
    "ext-sockets": "*",
    "ext-mysqli": "*",
    "evenement/evenement": "^3.0",
    "apix/log": "^1.2",
    "react/promise": "^2.9",
    "xiaosongshu/nacos": "dev-master",
    "workerman/workerman": "v4.1.9"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
